<style>

  body{
    font-size: 20px;
    margin: 0 auto;
    font-size: medium;
    background-color: #ffffff;
  }

</style>

<div>
  <div class="container">

    <div class="col-xs-8 col-xs-offset-2">
      <div class="row top-header">
        <div class="col-xs-12 text-center">
          <h2><%= bangla_number(@trade_organization.union.union_no.to_s) %> নং <%= @trade_organization.union.name_in_bng %> ইউনিয়ন পরিষদ</h2>
          <p class="sub">উপজেলাঃ <%= @trade_organization.union.upazila.name_in_bng %>
            জেলাঃ  <%= @trade_organization.union.upazila.district.name_in_bng %></p>
        </div>
      </div>

      <div class="row ">
        <div class="col-xs-12">
          <h2 class="sarker text-center">টাকা জমার রশিদ</h2>
        </div>
      </div>

      <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>সিরিয়াল নং</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.license_serial_no %></p>
          </div>
        </div>
      </div>
    </div>

      <div class="row">
        <div class="col-xs-12">
          <div class="row">
            <div class="col-xs-4 trade-label">
              <p>লাইসেন্স নং</p>
            </div>
            <div class="col-xs-8">
              <p><%= @trade_organization.license_no%></p>
            </div>
          </div>
        </div>
      </div>

    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>লাইসেন্স ইস্যু তারিখ</p>
          </div>
          <div class="col-xs-8">
            <p><%= bangla_date Time.now %></p>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>অর্থ বছর</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.fiscal_year %></p>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>প্রতিষ্ঠানের  নাম</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.enterprize_name_in_bng%></p>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p> প্রোপাইটর/মালিক</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.owners_name_bng %></p>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>ব্যবসার স্থান</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.business_place %></p>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>দোকান/হোল্ডিং নং</p>
          </div>
          <div class="col-xs-8">
            <p><%= bangla_number @trade_organization.holding_no %></p>
          </div>
        </div>
      </div>
    </div>


    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>হাল</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.license_fee %></p>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>বকেয়া</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.remain %></p>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
    <div class="col-xs-12">
      <div class="row">
        <div class="col-xs-4 trade-label">
          <p>জরিমানা/অন্যান্য</p>
        </div>
        <div class="col-xs-8">
          <p><%= @trade_organization.fine %></p>
        </div>
      </div>
    </div>
  </div>

    <div class="row">
      <div class="col-xs-12">
        <div class="row">
          <div class="col-xs-4 trade-label">
            <p>সর্বমোট</p>
          </div>
          <div class="col-xs-8">
            <p><%= @trade_organization.total_fee %></p>
          </div>
        </div>
      </div>
    </div>

      <div class="row trade-footer">
        <div class="col-xs-6">
        </div>

        <div class="col-xs-6">
          <div>
            <p class="text-center">সচিব</p>
            <p><%= bangla_number(@trade_organization.union.union_no.to_s)%> নং <%= @trade_organization.union.name_in_bng %> ইউনিয়ন পরিষদ</p>
          </div>
        </div>


      </div>

  </div>
  </div>
</div>