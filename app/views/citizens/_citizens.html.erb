<% if citizens %>
    <table class="table table-bordered table-striped">
      <thead>

      <tr>
        <th>Tracking Id</th>
        <th>NID</th>
        <th>Birth Id</th>
        <th>Name</th>
        <th>Father's Name</th>
        <th>Actions</th>
      </tr>
      </thead>
      <tbody>

      <% citizens.each do |citizen| %>
          <tr>
            <td><%= link_to(citizen.tracking_id, edit_citizen_path(citizen)) %></td>
            <td><%= link_to(citizen.status, edit_citizen_path(citizen)) %></td>
            <td><%= link_to(citizen.status, edit_citizen_path(citizen)) %></td>
            <td><%= link_to(citizen.status, edit_citizen_path(citizen)) %></td>
            <td><%= link_to(citizen.status, edit_citizen_path(citizen)) %></td>
            <td><%= link_to(citizen.status, edit_citizen_path(citizen)) %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
    <%= paginate citizens %>
<% end %>
