<div class="mainbox col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2">
  <p id="notice"><%= notice %></p>
  <h2>Money Collected</h2>

  <%= form_tag(collection_moneys_path, class: "form-inline", method: :get, remote: true) do %>

      <div class="form-group">
        <label class="sr-only" >Start Date</label>
        <%= text_field_tag :start_date, params[:start_Date], class: "form-control", placeholder: "Start Date" %>
      </div>

      <div class="form-group">
        <label class="sr-only" >End Date</label>
        <%= text_field_tag :end_date, params[:end_date], class: "form-control", placeholder: "Start Date" %>
      </div>
      <%= submit_tag :Search, class: "btn btn-primary" %>
  <% end %>

  <%= link_to 'pdf', citizens_path, class: "btn" %>


  <div id="results">
    <%= render 'results' %>
  </div>

</div>